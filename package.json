        "cordova-rm-browser": "cordova platform remove browser",
        "cordova-build-android": "npm run build && cordova build android",
        "cordova-build-ios": "npm run build && cordova build ios",
        "cordova-build-browser": "npm run build && cordova build browser",
        "cordova-run-browser": "npm run build && cordova run browser",
